issn = {03044238},
journal = {Scientia Horticulturae},
keywords = {Adaptation,Chilling requirements,Climatic change,Dormancy,Phenological Model,Phenology,Temperate fruit},
mendeley-tags = {Phenological Model},
month = {sep},
number = {2},
pages = {357--372},
pmid = {295298900001},
publisher = {Elsevier B.V.},
title = {{Dormancy in temperate fruit trees in a global warming context: A review}},
url = {https://www.sciencedirect.com/science/article/pii/S0304423811003694 http://dx.doi.org/10.1016/j.scienta.2011.07.011},
volume = {130},
