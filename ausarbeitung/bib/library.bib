@article{Liu2018,
abstract = {While climate warming reduces the occurrence of frost events, the warming-induced lengthening of the growing season of plants in the Northern Hemisphere may actually induce more frequent frost days during the growing season (GSFDs, days with minimum temperature {\textless} 0 °C). Direct evidence of this hypothesis, however, is limited. Here we investigate the change in the number of GSFDs at latitudes greater than 30° N using remotely-sensed and in situ phenological records and three minimum temperature (Tmin) data sets from 1982 to 2012. While decreased GSFDs are found in northern Siberia, the Tibetan Plateau, and northwestern North America (mainly in autumn), {\~{}}43{\%} of the hemisphere, especially in Europe, experienced a significant increase in GSFDs between 1982 and 2012 (mainly during spring). Overall, regions with larger increases in growing season length exhibit larger increases in GSFDs. Climate warming thus reduces the total number of frost days per year, but GSFDs nonetheless increase in many areas.},
author = {Liu, Qiang and Piao, Shilong and Janssens, Ivan A. and Fu, Yongshuo and Peng, Shushi and Lian, Xu and Ciais, Philippe and Myneni, Ranga B. and Pe{\~{n}}uelas, Josep and Wang, Tao},
doi = {10.1038/s41467-017-02690-y},
file = {:home/ines/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Liu et al. - 2018 - Extension of the growing season increases vegetation exposure to frost.pdf:pdf},
issn = {2041-1723},
journal = {Nature Communications},
keywords = {Climate,Phenological Model,Phenology,Sp{\"{a}}tfrost,Use of Climate Model,change ecology},
mendeley-tags = {Phenological Model,Sp{\"{a}}tfrost,Use of Climate Model},
month = {dec},
number = {1},
pages = {426},
publisher = {Nature Publishing Group},
title = {{Extension of the growing season increases vegetation exposure to frost}},
url = {http://www.nature.com/articles/s41467-017-02690-y},
volume = {9},
year = {2018}
}
@article{Bahuguna2015,
abstract = {Gradual and abrupt changes in temperature under current and future climates pose a serious threat to ecological diversity and in sustaining global food security. Plants possess a robust network of thermal sensors to program their metabolic and hormonal framework, providing acclimation to short-term abrupt fluctuations or adaptation to gradual temperature change. Several cellular thermosensors are reported in plants and understanding their mode of operation in a potentially coordinated framework continues to be a persistent challenge. Despite growing insight into the molecular control of thermal perception, physiological significance of plant thermal responses under a complex interaction with other environmental cues is largely unaddressed. Plant thermal perception and signaling could be similar across growth habits such as herbaceous to perennial woody species while developmental transitions such as vegetative to reproductive stage, influenced by their immediate microclimate, are generally altered across growth habits. The overall aim is to connect the knowledge gained with thermal perception and the signaling that drives plant responses, with major focus on the latter.},
author = {Bahuguna, Rajeev N. and Jagadish, Krishna S.V.},
doi = {10.1016/j.envexpbot.2014.10.007},
file = {::},
isbn = {0098-8472},
issn = {00988472},
journal = {Environmental and Experimental Botany},
keywords = {Cellular thermosensors,Hormones,Metabolites,Plant developmental decisions,Plant signaling,Thermal response},
pages = {83--90},
title = {{Temperature regulation of plant phenological development}},
volume = {111},
year = {2015}
}
