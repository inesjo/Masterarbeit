%% NOTE: In general, \patchcmd cannot be used in the argument of another command whenever the search and replacement texts
%% themselves include references to arguments. The inner \ifundef chain is needed to avoid an ‘Extra \fi’ error due to an
%% undefined \if@titlepage switch (That’s TeX!).
\ifundef\if@titlepage\iffalse\iftrue\ifundef\if@titlepage\iffalse%
  \if@titlepage
    \renewcommand*{\titlepagestyle}{%
      empty%
    }
    \patchcmd{\title}{%
      #1%
    }{%
      #1\thispagestyle{\titlepagestyle}%
    }{%
    }{%
      \PackageWarningFailedPatching{\@currname}{\title}%
    }
  \else
    \ifdefmacro{\scr@ext@activate@title}{%
      \appto\HyOrg@maketitle{%
        \thispagestyle{\titlepagestyle}%
      }%
    }{%
    }
  \fi
\fi


